import java.io.*;
import java.util.*;
public class Main {
	
	 public static int multiplyRecursively(int n, int m) {
		 if (m==1)
            return n;
		return multiplyRecursively(n,m-1) + n;
  }
 
  public static void main(String[] args) {
    Scanner scn = new Scanner(System.in);
    int n = scn.nextInt();
    int m = scn.nextInt();
    System.out.print(multiplyRecursively(n, m));
  }
}
