import java.util.*;

public class Main {
	static void printword(int n)
    {
        //Write your code here
		{
          // Your code here
		if(n==0)
		{
			return;
		}

		System.out.println("AccioSchool");

		printword(n-1);
    }
    }
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n= sc.nextInt();
	    printword(n);
    }
}
