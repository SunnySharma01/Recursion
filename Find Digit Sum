import java.util.*;
 
public class Main {
	static int sumOfDigi(int n) {
 
		if(n==0)
			return 0;
 
		return n%10 + sumOfDigi(n/10);
    }
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n;
        n = sc.nextInt();
        int result = sumOfDigi(n);
        System.out.println(result);
        sc.close();
    }
}
